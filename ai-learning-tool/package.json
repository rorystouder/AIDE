{
  "name": "ai-learning-tool",
  "displayName": "AI Learning Tool for Cursor",
  "description": "Advanced AI-powered development assistant with code analysis, performance optimization, security scanning, and personalized suggestions.",
  "version": "0.5.0",
  "publisher": "rorystouder",
  "repository": {
    "type": "git",
    "url": "https://github.com/rorystouder/AIDE"
  },
  "bugs": {
    "url": "https://github.com/rorystouder/AIDE/issues"
  },
  "homepage": "https://github.com/rorystouder/AIDE#readme",
  "license": "MIT",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Machine Learning",
    "Education",
    "Other"
  ],
  "keywords": [
    "cursor",
    "ai",
    "claude",
    "openai",
    "learning",
    "education",
    "code-generation",
    "assistant"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-learning-tool.helloWorld",
        "title": "AI Learning Tool: Hello World"
      },
      {
        "command": "ai-learning-tool.openChat",
        "title": "AI Learning Tool: Open AI Chat"
      },
      {
        "command": "ai-learning-tool.generateCode",
        "title": "AI Learning Tool: Generate Code"
      },
      {
        "command": "ai-learning-tool.switchProvider",
        "title": "AI Learning Tool: Switch AI Provider"
      },
      {
        "command": "ai-learning-tool.showCacheStats",
        "title": "AI Learning Tool: Show Cache Statistics"
      },
      {
        "command": "ai-learning-tool.clearCache",
        "title": "AI Learning Tool: Clear All Caches"
      },
      {
        "command": "ai-learning-tool.searchWorkspace",
        "title": "AI Learning Tool: Search Workspace"
      },
      {
        "command": "ai-learning-tool.findDefinitions",
        "title": "AI Learning Tool: Find Definitions"
      },
      {
        "command": "ai-learning-tool.findReferences",
        "title": "AI Learning Tool: Find References"
      },
      {
        "command": "ai-learning-tool.findTodos",
        "title": "AI Learning Tool: Find TODOs"
      },
      {
        "command": "ai-learning-tool.analyzeCode",
        "title": "AI Learning Tool: Analyze Code"
      },
      {
        "command": "ai-learning-tool.analyzePerformance",
        "title": "AI Learning Tool: Analyze Performance"
      },
      {
        "command": "ai-learning-tool.analyzeSecurity",
        "title": "AI Learning Tool: Analyze Security"
      },
      {
        "command": "ai-learning-tool.suggestRefactoring",
        "title": "AI Learning Tool: Suggest Refactoring"
      },
      {
        "command": "ai-learning-tool.showAnalytics",
        "title": "AI Learning Tool: Show Usage Analytics"
      },
      {
        "command": "ai-learning-tool.clearAnalytics",
        "title": "AI Learning Tool: Clear Analytics Data"
      },
      {
        "command": "ai-learning-tool.noCodeBuilder",
        "title": "AI Learning Tool: No-Code to Code Builder"
      },
      {
        "command": "ai-learning-tool.explainCode",
        "title": "AI Learning Tool: Explain Code Step-by-Step"
      },
      {
        "command": "ai-learning-tool.codingMentor",
        "title": "AI Learning Tool: Interactive Coding Mentor"
      },
      {
        "command": "ai-learning-tool.projectIdeas",
        "title": "AI Learning Tool: Generate Project Ideas"
      },
      {
        "command": "ai-learning-tool.debugHelper",
        "title": "AI Learning Tool: Debug Helper with Explanations"
      },
      {
        "command": "ai-learning-tool.noCodeBuilder", 
        "title": "AI Learning Tool: No-Code to Code Builder"
      },
      {
        "command": "ai-learning-tool.explainCode",
        "title": "AI Learning Tool: Explain Code Step-by-Step"
      }
    ],
    "configuration": {
      "title": "AI Learning Tool",
      "properties": {
        "ai-learning-tool.provider": {
          "type": "string",
          "enum": ["openai", "claude", "local", "cursor"],
          "default": "openai",
          "description": "AI provider to use for completions",
          "scope": "application"
        },
        "ai-learning-tool.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your API Key for the selected AI service. Not needed for 'cursor' or 'local' providers.",
          "scope": "application"
        },
        "ai-learning-tool.openai.apiUrl": {
          "type": "string",
          "default": "https://api.openai.com/v1/chat/completions",
          "description": "OpenAI API endpoint URL",
          "scope": "application"
        },
        "ai-learning-tool.openai.model": {
          "type": "string",
          "default": "gpt-3.5-turbo",
          "description": "OpenAI model to use",
          "scope": "application"
        },
        "ai-learning-tool.claude.apiUrl": {
          "type": "string",
          "default": "https://api.anthropic.com/v1/messages",
          "description": "Claude API endpoint URL",
          "scope": "application"
        },
        "ai-learning-tool.claude.model": {
          "type": "string",
          "default": "claude-3-haiku-20240307",
          "description": "Claude model to use (haiku for cost-effectiveness)",
          "scope": "application"
        },
        "ai-learning-tool.local.apiUrl": {
          "type": "string",
          "default": "http://localhost:11434/api/generate",
          "description": "Local AI API endpoint (e.g., Ollama)",
          "scope": "application"
        },
        "ai-learning-tool.local.model": {
          "type": "string",
          "default": "codellama:7b",
          "description": "Local model to use",
          "scope": "application"
        },
        "ai-learning-tool.cursor.integration": {
          "type": "boolean",
          "default": true,
          "description": "Enable Cursor-specific features and optimizations",
          "scope": "application"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.9.1",
    "glob": "^11.0.3",
    "sinon": "^18.0.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
